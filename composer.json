{
    "name": "fourello-devs/mr-speedy",
    "description": "Mr. Speedy Laravel-based SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "carlo.luchavez@fourello.com",
            "homepage": "https://www.fourello.com"
        }
    ],
    "homepage": "https://github.com/fourello-devs/mr-speedy",
    "keywords": ["Laravel", "MrSpeedy"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "files": [
            "helpers/CustomHelpers.php"
        ],
        "psr-4": {
            "FourelloDevs\\MrSpeedy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FourelloDevs\\MrSpeedy\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FourelloDevs\\MrSpeedy\\MrSpeedyServiceProvider"
            ],
            "aliases": {
                "MrSpeedy": "FourelloDevs\\MrSpeedy\\Facades\\MrSpeedy"
            }
        }
    }
}
